@model NewNationals.Models.ModelUrls
@{
    ViewBag.Title = Model.Title;
    string stCase = Model.ActionLink;
    switch (stCase)
    {
        case "Category":
            @Html.Action("Categories")
            break;
        case "Content":
            @Html.Action("Pages")
            break;
        case "Page":
            @Html.Action("DetailPage")
            break;
        case "404":
            @Html.Action("PageError")
            break;
        case "":
            @Html.Action("Index")
            break;
        default:
            @Html.Action("Index")
            break;
    }
}
